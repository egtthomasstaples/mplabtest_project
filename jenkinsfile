pipeline{
    agent any
    stages {
        stage('Checkout from SCM')
        {
            agent 
            {
                docker {image 'mplabx-5.25_xc16-1.36:latest'}    
            }
        }
        stage('Ls stuff')
        {
            agent
            {
                docker {image 'mplabx-5.25_xc16-1.36:latest'}
            }
            steps
            {
                sh 'ls'
            }
        }
    }
}